# Definitional proc to organize widgets for parameters.
proc init_gui { IPINST } {
  ipgui::add_param $IPINST -name "Component_Name"
  #Adding Page
  set Page_0 [ipgui::add_page $IPINST -name "Page 0"]
  set SERIALIZE_INTERFACES [ipgui::add_param $IPINST -name "SERIALIZE_INTERFACES" -parent ${Page_0}]
  set_property tooltip {Serializes the IO to get to 16 logical pins (external tri-state required). Uses SPI instead of an AXI4Lite regsiter map.} ${SERIALIZE_INTERFACES}
  ipgui::add_static_text $IPINST -name "Description" -parent ${Page_0} -text {Replaces the AXI4Lite register interface with a smaller SPI interface (CPHA=0, CPOL=0) and serializes the AXI Streams to a single bit of data.}


}

